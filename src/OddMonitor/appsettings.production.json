{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "log/odd-monitor_{Date}.txt",
          "retainedFileCountLimit": 7
        }
      },
      {
        "Name": "LiterateConsole"
      }
    ]
  }
}

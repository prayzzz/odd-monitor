<Project>
  <PropertyGroup>
    <Product>OddMonitor</Product>
    <RepositoryUrl>https://github.com/prayzzz/odd-monitor</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <Target Name="PrintVersion" BeforeTargets="BeforeBuild">
    <Message Text="Building $(AssemblyName) version $(Version)" Importance="High" />
    <WriteLinesToFile File="version" Lines="$(Version)" Overwrite="true" Encoding="Unicode"/>
  </Target>
  
</Project>